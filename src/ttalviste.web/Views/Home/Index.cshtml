@model ttalviste.web.Models.Home.IndexViewModel
@{
    ViewBag.Title = Model.PageTitle;
}
<!-- Intro Header -->
<header class="intro">
    <div class="intro-body">
        <div class="container">
            <div class="row">
                <div class="col-md-8 col-md-offset-2">
                    <h1 class="brand-heading">Sample</h1>
                    <p class="intro-text">Web and Systems development and consultancy</p>
                    <a href="#news" class="btn btn-circle page-scroll">
                        <i class="fa fa-angle-double-down animated"></i>
                    </a>
                </div>
            </div>
        </div>
    </div>
</header>
<section id="news" class="news-section container content-section text-center">
    @{
        await Html.RenderPartialAsync("_News");
    }
</section>
<!-- About Section -->
<section id="about" class="container content-section text-center">
    @{
        await Html.RenderPartialAsync("_About");
    }
</section>

<section id="contact" class="container content-section text-center">
    @{
        await Html.RenderPartialAsync("_Contact");
    }
</section>